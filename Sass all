@mixin style{
  .container{
    color:darkblue;
  }
}
@include style;

@mixin button{
  button{
  font-size:1em;
  padding:0.5em 1.0em;
  text-decoration:none;
  color:black;
    border:solid 1px;
    border-radius:5px;
}
  button:hover{
    cursor:pointer;
    background-color:white;
  }
}
@include button;
@mixin parent{
  font-size:1em;
  padding:0.5em 1.0em;
  text-decoration:none;
  color:black;
  font-weight:bolder;
}

.button-green{
  @include parent;
  background-color:green;
}

.button-red{
  @include parent;
  background-color:red;
}
@mixin button{
  font-size:1em;
  padding:0.5em 1.0em;
  text-decoration:none;
  color:black;
  font-weight:bolder;
}

@mixin link{
  a{
    color:blue;
    &:visited{color:purple;}
    &:hover{color:white;}
    &:active{color:red;}
  }
}

@mixin button-blue{
  .blue-button{
    @include button;
    @include link;
    background-color:blue;
    color:white;
  }
}

@include button-blue;
