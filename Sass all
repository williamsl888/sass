@mixin background-color-spectrum($base-color){
  @for $i from 1 through 4{
    &:nth-child({5-$i})
    {
      background-color:darken($base-color,$i*5%);
    }
    }
  &:nth-child(5){
    background-color:$base-color;
  }
  @for $i from 1 through 4{
    &:nth-child({$i+5})
    {
      background-color:lighten($base-color,$i*5%);
    }
  }
}

